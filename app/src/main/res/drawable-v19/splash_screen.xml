<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <color android:color="@color/wallet_bright_foreground_holo_dark" />
    </item>

    <item
        android:bottom="250dp"
        android:gravity="center_horizontal|center_vertical"
        android:left="25dp"
        android:right="25dp"
        android:top="250dp">
        <shape android:shape="oval">
            <solid android:color="@color/splashLogo" />
            <size
                android:width="240dp"
                android:height="110dp" />
        </shape>
    </item>

    <item
        android:bottom="240dp"
        android:gravity="center_horizontal|center_vertical"
        android:left="25dp"
        android:right="25dp"
        android:top="240dp">
        <inset
            android:drawable="@drawable/belvedere_logo"
            android:insetBottom="5dp"
            android:insetLeft="10dp"
            android:insetRight="10dp"
            android:insetTop="5dp" />
    </item>

</layer-list>